{
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "outputDirectory": "./",
  "installCommand": null,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
